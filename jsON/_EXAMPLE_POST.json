{
	"toast": {
		"text": "TOAST TEXT",
		"timeOut": 4000 // Hide Delay MS
	},
	"go": {
		"url": "forum-topic.html", // Redirect URL
		"delay": 1000 // Delay MS
	},
	"html": [
		{ "type": "append", "target": "form[name=topic-form]", "content": "<div class=\"alert alert-success\">Success...</div>", "escapeHTML": true, "delay": 1000 },
		{ "type": "dom", "target": "form[name=topic-form]", "content": "<div class=\"alert alert-success\">Success...</div>", "escapeHTML": true, "delay": 1000 },
		{ "type": "prepend", "target": "form[name=topic-form]", "content": "<div class=\"alert alert-success\">Success...</div>", "escapeHTML": true, "delay": 1000 },
		{ "type": "before", "target": "form[name=topic-form]", "content": "<div class=\"alert alert-success\">Success...</div>", "escapeHTML": true, "delay": 1000 },
		{ "type": "after", "target": "form[name=topic-form]", "content": "<div class=\"alert alert-success\">Success...</div>", "escapeHTML": true, "delay": 1000 },
		{ "type": "value", "target": "form[name=topic-form]", "text": "<div class=\"alert alert-success\">Success...</div>", "escapeHTML": true, "delay": 1000 }
	],
	"dom": [
		{ "type": "show", "target": ".target-selector", "delay": 1000 },
		{ "type": "hide", "target": ".target-selector", "delay": 1000 },
		{ "type": "remove", "target": ".target-selector", "delay": 1000 },
		{ "type": "reset", "target": ".target-selector", "delay": 1000 },
		{ "type": "appendTo", "element": ".element", "target": ".target-selector", "delay": 1000 },
		{ "type": "prependTo", "element": ".element", "target": ".target-selector", "delay": 1000 }
	],
	"editClass": [
		{ "target": ".target-selector", "add": "ion-android-favorite text-danger", "remove": "ion-android-favorite-outline", "addDelay": 1000, "removeDelay": 1000 }
	],
	"editCss": [
		{
			"target": "body", // Selector
			"css": [
				{
					"padding": "10px",
					"color": "#fff"
				}
			],
			"delay": 1000
		}
	],
	"modal": {
		"heading": "Modal Title",
		"body": "modal content",
		"footer": "modal footer",
		"delay": 1000,
		"closeDelay": 5000,
		"close": false,
		"class": "col-sm-4 col-sm-offset-4",
		"bodyClass": "bg-warning"
	},
	"pagination": {
		"total_results": 1064,
		"current_page": 50,
		"total_page": 107
	},
	"load": {
		"target": "this",
		"delay": 1000
	},
	"scrollTo": {
		"element": "#streaming",
		"tolerance": "-64px",
		"speed": 500,
		"delay": 1000
	},
	"run": [ "function1()", "function2()" ],
	"captchaReset": "--all-- or captcha_div_id"
}